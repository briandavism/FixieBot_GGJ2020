void setup() {
  pinMode(13,OUTPUT);
  display.begin(SSD1306_SWITCHCAPVCC, 0x3C);

  /*display.setTextColor(0xFFFF);
  display.setCursor(0,0);
  display.setTextSize(3);*/

}


void loop() {

  // Doing actions based on the active state
  if (state == IDLE) {
    Serial.print(state);
    playIdle();
  } else if (state == NEEDS_POWER) {
    Serial.print(state);
    playNeedsPower();
  } else if (state == BEING_POWERED) {
    Serial.print(state);
    playBeingPowered();
    // TODO: Draw the progress bar process to the screen
  } else if (state == POWERING_DONE) {
    Serial.print(state);
    playPoweringDone();
  } else if (state == NEEDS_TALKING) {
    Serial.print(state);
    playNeedsTalking();
  } else if (state == DECRYPTING_DONE) {
    Serial.print(state);
    playDecryptingDone();
  }

  // CONDITIONS & TRIGGERS FOR STATE CHANGES //

  // Add the key button to trigger a random change
  // if generate event button pressed (todo), generate random value between 0, 3(?), and activate the state
  if (generateEventPressed) {
    int eventNumber =  random(0, 1);
    if (eventNumber == 0) {
      state = NEEDS_POWER;
    }
    else if (eventNumber == 1) {
      state = NEEDS_TALKING;
    }
  }

  // TIME-BASED TEMPORARY TRIGGERS
  // Trigger needs power mode after 5 secs from start
  if (millis() > 5000 && testRunFinished == false) {
    state = NEEDS_POWER;
  }
  // Trigger powering mode after 10 secs from start
  if (millis() > 10000 && testRunFinished == false) {
    state = BEING_POWERED;
  }
  // Trigger powering done after 15 secs from start. Idle launches automatically afterwards
  if (millis() > 15000 && testRunFinished == false) {
    state = POWERING_DONE;
  }
  // Trigger powering done after 15 secs from start. Idle launches automatically afterwards
  if (millis() > 20000 && testRunFinished == false) {
    state = NEEDS_TALKING;
  } // Trigger powering done after 15 secs from start. Idle launches automatically afterwards
  if (millis() > 25000 && testRunFinished == false) {
    state = DECRYPTING_DONE;
  }
}

void drawBar(float value){
  display.fillRect(72,19,48*value/100,3,WHITE);
}


// ANIMATIONS //

void playIdle() {


  
  // Sample idle routine
  // Eyes open
  display.clearDisplay();
  display.drawBitmap(0, 0, eyes_open, 128, 64, WHITE);
  //display.print(F("Hello world"));
  
  display.display();
  delay(2000); // eyes open for value

  // Eyes blinking
  blinkEyes(500); // parameter: how long the eyes stay closed
  delay(500);
  blinkEyes(500);
}


// Closes eyes for the time of the duration and opens them
void blinkEyes(int blinkduration) {
  display.clearDisplay();
  display.drawBitmap(0, 0, eyes_closed, 128, 64, WHITE);
  display.display();
  delay(blinkduration);
  display.clearDisplay();
  display.drawBitmap(0, 0, eyes_open, 128, 64, WHITE);
  display.display();
}

void playNeedsPower() {
  // Sample needs power routine
