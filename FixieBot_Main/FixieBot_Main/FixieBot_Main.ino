/* POWER FUNCTIONS */
void increasePower() {
    if (state == NEEDS_POWER || state == BEING_POWERED) { // Only run if the state is needs power or being powered
      state = BEING_POWERED;                              // Switch state to being powered

      if (isIncreasingPower != true) {            // Check that the sound has been played till end until triggering the next event from the control
        if (stat_power < 100) {
          
          //Serial.println("Increased power");
          isIncreasingPower = true;               // Keeps global track of the function status, so that interrupts don't collide 
          stat_power = stat_power + 0.5;

          // Launches sound only every 10 updates
          encoderCount++;
          if (encoderCount > 10) {
            //playPoweredActionSound();           // Todo: This bugs for some reason... Try later
            encoderCount = 0;
          }
          // Returning status to false
          isIncreasingPower = false;
        }
    }
  }
}

void decreasePower() {
   if (stat_power > 0) {
    stat_power = stat_power - 0.5;

    // If the power ends up below zero, reset to zero
    if (stat_power < 0)
      stat_power = 0;
  }
}






