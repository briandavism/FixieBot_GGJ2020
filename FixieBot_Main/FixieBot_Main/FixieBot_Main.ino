void playDecryptingDone() {
  display.clearDisplay();
  display.drawBitmap(0, 0, decrypting_done, 128, 64, WHITE);
  display.display();
  delay(5000);
  // Returning to idle
  state = IDLE;
  testRunFinished = true;
}




